{
  "manifest_version": 3,
  "name": "FR Gouv. URLS Authenticator Checker",
  "description": "This extension is for education purpose",
  "version": "1.0",
  "action": {
    "notified_icon": "icon/icon_notified.png",
    "disabled_icon": "icon/icon_disabled.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "scripting",
    "activeTab"
